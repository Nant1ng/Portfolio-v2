{
  "name": "portfolio-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "sass assets/css/main.scss assets/css/main.css --style expanded",
    "minify:css": "sass assets/css/main.scss assets/css/main.min.css --style compressed",
    "build": "next build && npm run build:css && npm run minify:css",
